{
  "name": "@immowelt/workshop-js-fundamentals",
  "version": "0.0.0-development",
  "private": false,
  "author": "Immonet dev team <aegaeon@immonet.de> (https://immonet.de)",
  "license": "MIT",
  "description": "A living workshop around modern JavaScript and Flow with playground samples for the attendees.",
  "scripts": {
    "start": "serve . --open --clipless",
    "test": "npm run lint",
    "prettier": "prettier --write 'src/**/*.{js,json}'",
    "precommit": "lint-staged",
    "lint": "eslint src",
    "flow": "flow",
    "flow-typed": "flow-typed",
    "flow-typed-install": "flow-typed install --ignoreDeps=bundled peer --overwrite",
    "prepare": "npm run flow-typed-install || true"
  },
  "lint-staged": {
    "src/**/*.{js,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8.4.0",
    "npm": ">=5.3.0"
  },
  "devDependencies": {
    "@immowelt/babel-preset-immowelt": "^1.0.0",
    "@immowelt/eslint-config-immowelt-react": "^2.0.0",
    "@immowelt/styleguide-javascript": "^1.0.6",
    "eslint": "^4.4.1",
    "flow-bin": "^0.59.0",
    "flow-typed": "^2.1.2",
    "husky": "^0.14.3",
    "lint-staged": "^5.0.0",
    "opn-cli": "^3.1.0",
    "prettier": "^1.6.1"
  },
  "dependencies": {
    "code-mirror-themes": "^1.0.0",
    "codemirror": "^5.29.0",
    "plow-js": "^2.1.0",
    "serve": "^6.0.6"
  }
}
